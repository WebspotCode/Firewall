{
    "name": "webspot/firewall",
    "type": "library",
    "description": "Webspot Firewall with StackPHP support",
    "homepage": "http://webspotframework.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Jelmer Schreuder",
            "email": "j.schreuder@webspotframework.com"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "symfony/http-foundation": "~2.1",
        "symfony/event-dispatcher": "~2.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "symfony/http-kernel": "~2.1",
        "psecio/jwt": "~1.0"
